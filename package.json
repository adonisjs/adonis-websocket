    "lint": "standard",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "websocket",
    "socket"
  ],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.8",
    "@adonisjs/mrm-preset": "^1.0.9",
    "@adonisjs/sink": "^1.0.16",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^12.0.2",
    "pkg-ok": "^2.2.0",
    "standard": "^11.0.1",
    "test-console": "^1.1.0"
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "@adonisjs/middleware-base": "^1.0.0",
    "@adonisjs/websocket-packet": "^1.0.6",
    "cuid": "^2.1.1",
    "debug": "^3.1.0",
    "emittery": "^0.4.1",
    "macroable": "^1.0.0",
    "ws": "^5.2.2"
  },
  "nyc": {
    "exclude": [
      "test",
      "japaFile.js"
    ]
  },
  "standard": {
    "ignore": [
      "config"
    ]
