{
  "name": "@adonisjs/websocket",
  "version": "1.0.12",
  "description": "websocket server for Adonis framework",
  "main": "index.js",
  "files": [
    "config",
    "providers",
    "src",
    "clusterPubSub.js",
    "instructions.js",
    "instructions.md"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
