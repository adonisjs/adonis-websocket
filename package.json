{
  "name": "@adonisjs/websocket",
  "version": "1.0.9",
  "description": "websocket server for Adonis framework",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc japa",
    "test:win": "node ./node_modules/japa-cli/index.js"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "websocket",
    "socket"
  ],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.7",
    "@adonisjs/framework": "^5.0.5",
    "@adonisjs/sink": "^1.0.16",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^12.0.1",
    "standard": "^11.0.1",
    "test-console": "^1.1.0"
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.0",
    "@adonisjs/middleware-base": "^1.0.0",
    "@adonisjs/websocket-packet": "^1.0.6",
    "cuid": "^2.1.0",
    "debug": "^3.1.0",
    "emittery": "^0.3.0",
    "macroable": "^1.0.0",
    "ws": "^5.0.0"
  },
  "nyc": {
    "exclude": [
      "src/Context/index.js",
      "src/ClusterHop/index.js",
      "src/ClusterHop/sender.js",
      "clusterPubSub.js",
      "test",
      "config"
    ]
  },
  "standard": {
    "ignore": ["config"]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
